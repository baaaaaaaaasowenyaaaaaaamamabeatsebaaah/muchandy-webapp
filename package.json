{
  "name": "muchandy-webapp",
  "version": "1.0.0",
  "description": "Muchandy website built with Svarog UI and Storyblok CMS",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "start": "node server.js",
    "db:setup": "npx prisma generate && npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    "crawl": "node -e \"import('./src/services/simpleCrawler.js').then(({crawlPrices}) => crawlPrices())\"",
    "crawl:test": "node -e \"import('./src/services/simpleCrawler.js').then(({crawlPrices}) => crawlPrices()).catch(console.error)\"",
    "setup": "npm install && npm run db:setup && npm run crawl",
    "fresh": "npm run db:reset && npm run crawl && npm run dev",
    "lint": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.{js,css,html}",
    "validate": "npm run lint && npm run format"
  },
  "keywords": [
    "muchandy",
    "svarog-ui",
    "storyblok",
    "vanilla-javascript",
    "spa"
  ],
  "author": "Sebastian Huber",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@svarog-ui/theme-muchandy": "^1.9.2",
    "express": "^5.1.0",
    "prisma": "^6.9.0",
    "puppeteer": "^24.10.1",
    "storyblok-js-client": "^6.0.0",
    "svarog-ui-core": "^2.7.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-prettier": "^5.4.1",
    "prettier": "^3.2.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
